<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <LangVersion>8</LangVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>ConsoleTable</Title>
        <Authors>Oleksii Kozhemiaka</Authors>
        <Description>A library for simple table formatting</Description>
        <PackageProjectUrl>https://github.com/sakozoko/ConsoleTable</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/sakozoko/ConsoleTable/blob/master/Licence.MD</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/sakozoko/ConsoleTable</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>ConsoleTable, ct, table, console</PackageTags>
        <Company>Oleksii Kozhemiaka</Company>
        <AssemblyVersion>1.0.0</AssemblyVersion>
        <FileVersion>1.0.0</FileVersion>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    </PropertyGroup>

</Project>
